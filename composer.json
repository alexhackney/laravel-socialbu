{
    "name": "alexhackney/laravel-socialbu",
    "description": "Publish and schedule social media posts via SocialBu in Laravel",
    "keywords": ["laravel", "socialbu", "social-media", "api", "scheduling"],
    "homepage": "https://github.com/alexhackney/laravel-socialbu",
    "license": "MIT",
    "authors": [
        {
            "name": "Hackney Enterprises Inc",
            "email": "dev@hei.software"
        }
    ],
    "support": {
        "issues": "https://github.com/alexhackney/laravel-socialbu/issues",
        "source": "https://github.com/alexhackney/laravel-socialbu"
    },
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Hei\\SocialBu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hei\\SocialBu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hei\\SocialBu\\SocialBuServiceProvider"
            ],
            "aliases": {
                "SocialBu": "Hei\\SocialBu\\Facades\\SocialBu"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
